<?xml version="1.0" encoding="utf-8"?>
<workbook>
	<worksheet name="Completo">
		<loop startRow="4" endRow="4" items="container.rows" var="row" varType="rf.upload.mapping.MapTariffari">
			<section startRow="0" endRow="0">
				<mapping row="1" col="0">row.categoria</mapping>
				<mapping row="1" col="1">row.codice</mapping>
				<mapping row="1" col="2">row.denominazione</mapping>
				<mapping row="1" col="3">row.prezzoAlKilo</mapping>
				<mapping row="1" col="4">row.prezzoAlPezzo</mapping>
				<mapping row="1" col="5">row.tipoConfezione</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck>
					<cellcheck />
				</rowcheck>
			</loopbreakcondition>
		</loop>
		
	</worksheet>

</workbook>